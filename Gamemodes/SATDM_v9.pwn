public OnGangCaptureZone( Gang:gangid, Zone:zoneid, Gang:against )
{
	new str[ 255 ];
	format( str, sizeof(str), "NEWS: The %s gang captured the %s zone from the %s gang.", Gang_ReturnName( gangid ), Zone_ReturnName( zoneid ), Gang_ReturnName( against ) );
	SendClientMessageToAll( COLOR_RED, str );

	Zone_ToggleFlashing( zoneid, 0 );
	Zone_SetOwner( zoneid, gangid );
	Zone_SetColor( zoneid, (Gang_GetColor( gangid ) << 8) | 0x96 );

	GangScores[ gangid ][ zoneid ] = 0;
	GangScores[ against ][ zoneid ] = 0;
}


public OnGangAttackZone( Gang:gangid, Zone:zoneid, Gang:against )
{
	new str[ 255 ];
	format( str, sizeof(str), "NEWS: The %s gang attacked the %s gang's %s zone.", Gang_ReturnName( gangid ), Gang_ReturnName( against ), Zone_ReturnName( zoneid ) );
	SendClientMessageToAll( COLOR_RED, str );

	Zone_ToggleFlashing( zoneid, 1 );
	Zone_Attack( zoneid, gangid );

	GangScores[ gangid ][ zoneid ] = 0;
	GangScores[ against ][ zoneid ] = 0;
}

public OnGangDefendZone( Gang:gangid, Zone:zoneid, Gang:against )
{
	new str[ 255 ];
	format( str, sizeof(str), "NEWS: The %s gang defended the %s zone from the %s gang.", Gang_ReturnName( gangid ), Zone_ReturnName( zoneid ), Gang_ReturnName( against ) );
	SendClientMessageToAll( COLOR_RED, str );

	Zone_ToggleFlashing( zoneid, 0 );
	Zone_CancelAttack( zoneid );

	GangScores[ gangid ][ zoneid ] = 0;
	GangScores[ against ][ zoneid ] = 0;
}

