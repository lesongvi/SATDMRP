stock GetDriverID(vehicleid)
{
	for(new i=0; i<MAX_VEHICLES; i++) {
		if(IsPlayerConnected(i)) {
			if(IsPlayerInAnyVehicle(i)) {
				if(GetPlayerVehicleID(i) == vehicleid) {
					return i;
				}
			}
		}
	}
	return -1;
}
