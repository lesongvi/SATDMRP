	if(CountDown==0)
	{
		GameTextForAll("~g~GO~ r~!",1000,6);
		KillTimer(CountTimer);
		CountDown = -1;
		for(new i = 0; i < MAX_PLAYERS; i++) {
			TogglePlayerControllable(i,1);
			PlayerPlaySound(i, 1057, 0.0, 0.0, 0.0);
		}
	}
}

forward Duel(player1, player2);
public Duel(player1, player2)
