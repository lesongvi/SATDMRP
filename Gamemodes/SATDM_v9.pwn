	if(PlayerInfo[playerid][Level] ==5) {
		new ID = HouseID++;
		new Float:X, Float:Y, Float:Z;
		new File:handle = fopen(SaveFile, io_append);
		GetPlayerPos(playerid, X, Y, Z);
		format(string, sizeof(string), "AddHouse(%d, %f, %f, %f, 2251.85, -1138.16, 1050.63, 50000, 49000, 9, 0); //A small house ;)\r\n", ID, X, Y, Z);
