public countdown()
{
	if(CountDown==6) GameTextForAll("~p~Starting...",1000,6);

	CountDown--;
	new text[20]; format(text,sizeof(text),"~w~%d",CountDown);
	for(new i = 0; i < MAX_PLAYERS; i++) { PlayerPlaySound(i, 1056, 0.0, 0.0, 0.0);	TogglePlayerControllable(i,0); }
