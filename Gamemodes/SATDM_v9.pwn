		SendClientMessage(playerid, COLOR_ERROR, "You are dead. You cannot use this command");
		return 1;
	}
	if(PlayerInfo[playerid][team] != TEAM_LVCOP && PlayerInfo[playerid][team] != TEAM_LSCOP && PlayerInfo[playerid][team] != TEAM_SWAT && PlayerInfo[playerid][team] != TEAM_ARMY && PlayerInfo[playerid][team] != TEAM_LVARMY && PlayerInfo[playerid][Level] == 0)
