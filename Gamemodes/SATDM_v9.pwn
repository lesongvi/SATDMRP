CMD:blackadd(playerid,params[]) {
	new tmp[128];
	if (sscanf(params, "u", tmp)) return SendClientMessage(playerid, COLOR_LIGHTBLUE, "USAGE: /blackadd <PlayerName>");
	if(IsPlayerAdmin(playerid) && PlayerInfo[playerid][Level] == 5)
	{
		if(AddNameToBlacklist(tmp)) JB_SendFormattedMessage(playerid,GREEN,"JunkBuster: Player '%s' has successfully been added to blacklist!",tmp);
		else JB_SendFormattedMessage(playerid,RED,"JunkBuster: Could not add player '%s' to blacklist!",tmp);
	}
	return 1;
}

