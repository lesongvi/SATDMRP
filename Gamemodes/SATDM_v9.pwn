// ------------------------------Car Whisper------------------------------------
CMD:cw(playerid,params[]) {
 	new msg[128];
	if (sscanf(params, "s", msg)) return SendClientMessage(playerid, COLOR_LIGHTBLUE, "USAGE: /cw <Message>");
	if(!IsPlayerInAnyVehicle(playerid)) return SendClientMessage(playerid, COLOR_RED, "You must be in a car to use this command!");
	new pstate[128];
	switch(GetPlayerState(playerid))
	{
		case PLAYER_STATE_DRIVER: pstate = "Driver";
		case PLAYER_STATE_PASSENGER: pstate = "Passenger";
	}
	new pname[128];
	GetPlayerName(playerid, pname, 128);
	format(msg, 128, "%s %s [ID:%d]: %s", pstate, pname, playerid, msg);
	for(new i = 0; i < GetMaxPlayers(); i++)
	{
		if(IsPlayerInVehicle(i, GetPlayerVehicleID(playerid)))
		{
			SendClientMessage(i, COLOR_BLUE, msg);
		}
	}
	return 1;
}
