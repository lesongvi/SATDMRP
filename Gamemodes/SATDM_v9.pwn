CMD:bestscores(playerid,params[]) {
	new bestplayerid,bestscore;
	new dname[MAX_PLAYER_NAME];
	new string[128];

	GetBestScore(bestplayerid,bestscore);
	GetPlayerName(bestplayerid,dname,sizeof(dname));
	format(string,sizeof(string),"%s is now the lead with %d points.",dname,bestscore);
	SendClientMessage(playerid,0xFF8A00AA,string);

	GetBestScore(bestplayerid,bestscore,bestscore);
	GetPlayerName(bestplayerid,dname,sizeof(dname));
	format(string,sizeof(string),"%s is now second with %d points.",dname,bestscore);
	SendClientMessage(playerid,0xFF8A00AA,string);

	GetBestScore(bestplayerid,bestscore,bestscore);
	GetPlayerName(bestplayerid,dname,sizeof(dname));
	format(string,sizeof(string),"%s is now third with %d points.",dname,bestscore);
	SendClientMessage(playerid,0xFF8A00AA,string);
	return 1;
}
