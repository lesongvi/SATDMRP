//=================================AZONE========================================
CMD:azone(playerid,params[]) {
	new result[128], idx,color[128], latency, zones, command[128];
	if(PlayerInfo[playerid][Level]<5) return SendClientMessage(playerid, COLOR_RED, "That command is for admins ONLY!");
	if (sscanf(params, "s ", command) != 0)
	{
		SendClientMessage(playerid, COLOR_RED, "Usage: /azone <create/destroy/lock/unlock/color/minX/minY/maxX/maxY/flash/visible>");
		return 1;
	}
 	if (strcmp("create", command, true, 6) == 0)
	{
		if(sscanf(params, "ss", command, result)!=0){
			SendClientMessage( playerid, COLOR_YELLOW, "/AZONE CREATE <NAME>" );
			return 1;
		}
		new Float:ppos[ 3 ];
		GetPlayerPos( playerid, ppos[ 0 ], ppos[ 1 ], ppos[ 2 ] );
		new Zone:zoneid = Zone_Add( result, ppos[ 0 ] - 50.0, ppos[ 1 ] - 50.0, ppos[ 0 ] + 50.0, ppos[ 1 ]+ 50.0, 0x00FF0096 );
		if( zoneid == ZONE_INVALID ){
		    SendClientMessage( playerid, COLOR_RED, "ERROR: You canot create more zones");
		}
		else {
		    new zcstring[ 128 ];
		    format( zcstring, sizeof(zcstring), "You've succesfully created the %s zone (id: %i)", result, _:zoneid );
		    SendClientMessage( playerid, COLOR_GREEN, zcstring );
		}
		return 1;
	}
 	else if (strcmp("destroy", command, true, 7) == 0)
	{
		if(sscanf(params, "si", command, zones)!=0){
			SendClientMessage( playerid, COLOR_YELLOW, "/AZONE DESTROY <ZONEID>" );
			return 1;
		}
		if( !Zone_IsActive( Zone:zones ) ){
		    SendClientMessage( playerid, COLOR_RED, "ERROR: This zone is inactive" );
		}
		else {
		    new zdstring[128], zname[128];
		    Zone_GetName( Zone:zones, zname, MAX_ZONE_NAME );
		    format( zdstring, sizeof(zdstring), "You've succesfully deleted the %s zone (id: %i)", zname, _:zones );
		    SendClientMessage(playerid, COLOR_GREEN, zdstring);
		    Zone_Remove(Zone:zones);
		}
		return 1;
	}
 	else if (strcmp("lock", command, true, 4) == 0)
	{
		if(sscanf(params, "si", command, zones)!=0){
			SendClientMessage( playerid, COLOR_YELLOW, "/AZONE LOCK <ZONEID>" );
			return 1;
		}
		if(!Zone_IsActive(Zone:zones)){
		    SendClientMessage( playerid, COLOR_RED, "ERROR: This zone is inactive" );
		}
		else {
		    new zlstring[128], lname[128];
		    Zone_GetName( Zone:zones, lname, MAX_ZONE_NAME );
		    format( zlstring, sizeof(zlstring), "You've succesfully locked the %s zone (id: %i)", lname, _:zones );
		    SendClientMessage( playerid, COLOR_GREEN, zlstring );
		    Zone_ToggleLocked( Zone:zones, true );
		}
		return 1;
	}
 	else if (strcmp("unlock", command, true, 6) == 0)
	{
		if(sscanf(params, "si", command, zones)!=0){
			SendClientMessage( playerid, COLOR_YELLOW, "/AZONE UNLOCK <ZONEID>" );
			return 1;
		}
		if( !Zone_IsActive( Zone:zones ) ){
		    SendClientMessage( playerid, COLOR_RED, "ERROR: This zone is inactive" );
		}
		else {
			new zulstring[128], ulname[128];
		    Zone_GetName( Zone:zones, ulname, MAX_ZONE_NAME );
		    format( zulstring, sizeof(zulstring), "You've succesfully unlocked the %s zone (id: %i)", ulname, _:zones );
		    SendClientMessage( playerid, COLOR_GREEN, zulstring );
		    Zone_ToggleLocked( Zone:zones, false );
		}
		return 1;
	}
 	else if (strcmp("color", command, true, 5) == 0)
	{
		if(sscanf(params, "sis", command, zones, color)!=0){
			SendClientMessage( playerid, COLOR_YELLOW, "/AZONE COLOR <ZONEID> <COLOR(Choose from /zcolors)>" );
			return 1;
		}
		if( !Zone_IsActive( Zone:zones ) ){
		    SendClientMessage( playerid, COLOR_RED, "ERROR: This zone is inactive" );
		}
		else {
			idx = GetColorFromName(color);
			if( idx == -1 ){
			    SendClientMessage( playerid, COLOR_RED, "ERROR: Color not found(Choose from /zcolors)" );
			    return 1;
			}
			Zone_SetColor( Zone:zones, idx );
		}
		return 1;
	}
 	else if (strcmp("minx", command, true, 4) == 0)
	{
		if(sscanf(params, "sii", command, zones, latency)!=0){
			SendClientMessage( playerid, COLOR_YELLOW, "/AZONE MINX <ZONEID> <VALUE(-100..100)>" );
			return 1;
		}
		if( !Zone_IsActive( Zone:zones ) ){
		    SendClientMessage( playerid, COLOR_RED, "ERROR: This zone is inactive" );
		}
		else {
			if( latency < -100.0 || latency > 100.0 ){
			    SendClientMessage( playerid, COLOR_RED, "ERROR: Invalid value (-100..100)" );
				return 1;
			}
			new Float:zzpos[ 4 ];
			Zone_GetPos( Zone:zones, zzpos[ 0 ], zzpos[ 1 ], zzpos[ 2 ], zzpos[ 3 ] );
			Zone_SetPos( Zone:zones, (zzpos[ 0 ] + latency), zzpos[ 1 ], zzpos[ 2 ], zzpos[ 3 ] );
		}
		return 1;
	}
 	else if (strcmp("miny", command, true, 4) == 0)
	{
		if(sscanf(params, "sii", command, zones, latency)!=0){
			SendClientMessage( playerid, COLOR_YELLOW, "/AZONE MINY <ZONEID> <VALUE(-100..100)>" );
			return 1;
		}
		if( !Zone_IsActive( Zone:zones ) ){
		    SendClientMessage( playerid, COLOR_RED, "ERROR: This zone is inactive" );
		}
		else {
			if( latency < -100.0 || latency > 100.0 ){
			    SendClientMessage( playerid, COLOR_RED, "ERROR: Invalid value (-100..100)" );
				return 1;
			}
			new Float:zzpos[ 4 ];
			Zone_GetPos( Zone:zones, zzpos[ 0 ], zzpos[ 1 ], zzpos[ 2 ], zzpos[ 3 ] );
			Zone_SetPos( Zone:zones, zzpos[ 0 ], (zzpos[ 1 ] + latency), zzpos[ 2 ], zzpos[ 3 ] );
		}
		return 1;
	}
 	else if (strcmp("maxx", command, true, 4) == 0)
	{
		if(sscanf(params, "sii", command, zones, latency)!=0){
			SendClientMessage( playerid, COLOR_YELLOW, "/AZONE MAXX <ZONEID> <VALUE(-100..100)>" );
			return 1;
		}
		if( !Zone_IsActive( Zone:zones ) ){
		    SendClientMessage( playerid, COLOR_RED, "ERROR: This zone is inactive" );
		}
		else {
			if( latency < -100.0 || latency > 100.0 ){
			    SendClientMessage( playerid, COLOR_RED, "ERROR: Invalid value (-100..100)" );
				return 1;
			}
			new Float:zzpos[ 4 ];
			Zone_GetPos( Zone:zones, zzpos[ 0 ], zzpos[ 1 ], zzpos[ 2 ], zzpos[ 3 ] );
			Zone_SetPos( Zone:zones, zzpos[ 0 ], zzpos[ 1 ], (zzpos[ 2 ] + latency), zzpos[ 3 ] );
		}
		return 1;
	}
 	else if (strcmp("maxy", command, true, 4) == 0)
	{
		if(sscanf(params, "sii", command, zones, latency)!=0){
			SendClientMessage( playerid, COLOR_YELLOW, "/AZONE MAXY <ZONEID> <VALUE(-100..100)>" );
			return 1;
		}
		if( !Zone_IsActive( Zone:zones ) ){
		    SendClientMessage( playerid, COLOR_RED, "ERROR: This zone is inactive" );
		}
		else {
			if( latency < -100.0 || latency > 100.0 ){
			    SendClientMessage( playerid, COLOR_RED, "ERROR: Invalid value (-100..100)" );
				return 1;
			}
			new Float:zzpos[ 4 ];
			Zone_GetPos( Zone:zones, zzpos[ 0 ], zzpos[ 1 ], zzpos[ 2 ], zzpos[ 3 ] );
			Zone_SetPos( Zone:zones, zzpos[ 0 ], zzpos[ 1 ], zzpos[ 2 ], (zzpos[ 3 ] + latency) );
		}
		return 1;
	}
 	else if (strcmp("flash", command, true, 4) == 0)
	{
		if(sscanf(params, "sii", command, zones, latency)!=0){
			SendClientMessage( playerid, COLOR_YELLOW, "/AZONE FLASH <ZONEID>" );
			return 1;
		}
		if( !Zone_IsActive( Zone:zones ) ){
		    SendClientMessage( playerid, COLOR_RED, "ERROR: This zone is inactive" );
		}
		else {
		    if( Zone_IsFlashing( Zone:zones ) ) Zone_ToggleFlashing( Zone:zones, false );
		    else Zone_ToggleFlashing( Zone:zones, true );
		}
		return 1;
	}
 	else if (strcmp("visible", command, true, 4) == 0)
	{
		if(sscanf(params, "sii", command, zones, latency)!=0){
			SendClientMessage( playerid, COLOR_YELLOW, "/AZONE VISIBLE <ZONEID>" );
			return 1;
		}
		if( !Zone_IsActive( Zone:zones ) ){
		    SendClientMessage( playerid, COLOR_RED, "ERROR: This zone is inactive" );
		}
		else {
		    if( Zone_IsVisible( Zone:zones ) ) Zone_ToggleVisible(Zone:zones, false );
		    else Zone_ToggleVisible( Zone:zones, true );
		}
		return 1;
	}
	return 1;
}

