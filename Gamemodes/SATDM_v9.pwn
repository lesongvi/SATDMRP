stock split(const strsrc[], strdest[][], delimiter)
{
	new i, li;
	new aNum;
	new slen;

	while(i <= strlen(strsrc)){
		if(strsrc[i]==delimiter || i==strlen(strsrc)){
			slen = strmid(strdest[aNum], strsrc, li, i, 128);
			strdest[aNum][slen] = 0;
			li = i+1;
			aNum++;
		}
		i++;
	}
	return 1;
}

