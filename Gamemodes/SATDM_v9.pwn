CMD:tban(playerid,params[]) {
	new tmp[128],time,pid;
	if (sscanf(params, "uis", pid,time,tmp)) return SendClientMessage(playerid, COLOR_LIGHTBLUE, "USAGE: /tban <ID> <time in hours> <reason>");
	if(IsPlayerAdmin(playerid) && PlayerInfo[playerid][Level] == 5)
	{
		if(IsPlayerConnected(pid))
		{
			TempBan(pid,time,tmp);
		}
	}
	return 1;
}

CMD:tunban(playerid,params[]) {
	new tmp[128];
	if (sscanf(params, "u",tmp)) return SendClientMessage(playerid, COLOR_LIGHTBLUE, "USAGE: /tunban <Name>");
	if(IsPlayerAdmin(playerid) && PlayerInfo[playerid][Level] == 5)
	{
		if(DeleteTempBan(tmp)) JB_SendFormattedMessage(playerid,GREEN,"JunkBuster: Temporary ban of player '%s' has successfully been deleted.",tmp);
		else JB_SendFormattedMessage(playerid,RED,"JunkBuster: Could not delete temporary ban of player '%s'!",tmp);
	}
	return 1;
}

CMD:banip(playerid,params[]) {
	new tmp[4];
	if (sscanf(params, "i",tmp)) return SendClientMessage(playerid, COLOR_LIGHTBLUE, "USAGE: /banip <IP>");
	if(IsPlayerAdmin(playerid) && PlayerInfo[playerid][Level] == 5)
	{
		new ip[4];
		ip=Split(tmp);
