stock strequal( const str1[], const str2[], bool:ignorecase = true )
{
	if( strlen( str1 ) == strlen( str2 ) && !strcmp( str1, str2, ignorecase ) ) return 1;
	return 0;
}


stock GetColorFromName( const string[] )
{
	if( strequal( string, "WHITE" ) ) return 0xFFFFFF96;
	if( strequal( string, "BLACK" ) ) return 0x00000096;
	if( strequal( string, "RED" ) ) return 0xFF000096;
	if( strequal( string, "LIME" ) ) return 0x00FF0096;
	if( strequal( string, "GREEN" ) ) return 0x009F0096;
	if( strequal( string, "ORANGE" ) ) return 0xFFA50096;
	if( strequal( string, "PINK" ) ) return 0xFF00FF96;
	if( strequal( string, "YELLOW" ) ) return 0xFFFF0096;
	if( strequal( string, "TEAL" ) ) return 0x00FFFF96;
	if( strequal( string, "PURPLE" ) ) return 0x80008096;
	if( strequal( string, "BLUE" ) ) return 0x0000FF96;
	if( strequal( string, "GOLD" ) ) return 0xFFCC0096;
	return -1;
}

